{
    "rust-analyzer.cargo.features": [
        "diffsol-llvm17",
    ],
    "rust-analyzer.cargo.extraEnv": {
        "SUNDIALS_LIBRARY_DIR": "~/.local/lib",
        "SUNDIALS_INCLUDE_DIR": "~/.local/include",
        "LLVM_DIR": "/usr/lib/llvm-17",
        "LLVM_SYS_170_PREFIX": "/usr/lib/llvm-17",
    },
    "rust-analyzer.files.excludeDirs": [
        "**/.git",
        "**/.vscode",
        "**/target",
        "**/env",
        "**/doctest_cache",
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/target/**": true,
        "**/env/**": true,
        "**/doctest_cache/**": true,
    }
}